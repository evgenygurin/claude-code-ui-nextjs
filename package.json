    "codegen:cleanup": "node scripts/task-scheduler.js cleanup",
    "auth:check": "node scripts/check-auth-tokens.js",
    "auth:setup": "node scripts/setup-vercel-config.js setup",
    "auth:monitor": "node scripts/auth-monitor.js check",
    "auth:monitor:continuous": "node scripts/auth-monitor.js start",
    "auth:status": "node scripts/auth-monitor.js status"
