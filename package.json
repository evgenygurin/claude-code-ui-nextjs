{
  "name": "claude-code-ui-nextjs",
  "version": "1.0.0",
  "description": "Modern web UI for Claude Code CLI and Cursor CLI built with Next.js",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "clean": "rm -rf .next out coverage",
    "preview": "next build && next start",
